		"fnRowCallback": function( nRow, aData, iDisplayIndex ) {
						if ((aData[5] == "101" && aData[9] == "100501111") || (aData[5] == "101" && aData[9] == "100501112") || aData[7] == "100" || aData[5] == "100")
